import sympy
from sympy import *
init_printing(use_unicode=True)
#t,e = symbols('t e') 
t = Symbol('t')
exps = (2*t) + (exp(t/2)/2) + (sinh(t+1))
pprint(simplify(exps))

trail1 = integrate(exps, t) 
print("Position: ",trail1.subs(t, 0.9))

print("Velocity= ",exps.subs(t,0.9))

trail2 = diff(exps, t) 
print("Acceleration: ",trail2.subs(t,0.9))

